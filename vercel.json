{
    "version": 2,
    "builds": [
        {
            "src": "dist/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server.js",
            "headers": {
                "Access-Control-Allow-Origin": "http://localhost:3000"
            }
        }
    ]
}